{

  "ConnectionStrings": {
    "DefaultConnection": "localhost:postgres:5432"
  },
    "Authentication": {
        "Audience": "account",
        "TokenValidationParameters": {
            "ValidIssuers": [ "https://www.google.com/", "http://localhost:18080/realms/ewaste/" ]
        },
        "MetadataAddress": "http://EWaste.Identity:18080/realms/ewaste/.well-known/openid-configuration",
        "Secret": "YourSuperSecretKeyHereAtLeast32CharsLongeregdfvbdfgdfladwas"
    },
    "Serilog": {
        "Using": [ "Serilog.Sinks.File" ],
        "MinimumLevel": {
            "Default": "Information"
        },
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "./Log/log-.txt",
                    "rollingInterval": "Day",
                    "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {CorrelationId} {Level:u3}] {Username} {Message:lj}{NewLine}{Exception}"
                }
            }
        ]
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}
